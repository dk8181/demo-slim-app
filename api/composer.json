{
    "name": "mdv/demo-auction",
    "type": "project",
    "license": "BSD-3-Clause",
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "require": {
        "ext-json": "^8.0",
        "php": "^8.0",
        "php-di/php-di": "^6.3",
        "ramsey/uuid": "^4.2",
        "slim/slim": "^4.9",
        "slim/psr7": "^1.5",
        "symfony/console": "^5.4",
        "webmozart/assert": "1.10"
    },
    "require-dev": {
        "overtrue/phplint": "^4.0",
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.6",
        "vimeo/psalm": "^4.15"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/"
        }
    },
    "scripts": {
        "lint": "phplint",
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "psalm": "psalm",
        "test": "phpunit --colors=always",
        "test-coverage": "phpunit --colors=always --coverage-html var/phpunit/coverage",
        "app": "php bin/app.php --ansi"
    }
}

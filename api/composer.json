{
    "name": "mdv/demo-auction",
    "type": "project",
    "license": "BSD-3-Clause",
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "require": {
        "ext-json": "^8.0",
        "php": "^8.0",
        "php-di/php-di": "^6.3",
        "slim/slim": "^4.9",
        "slim/psr7": "^1.5",
        "symfony/console": "^5.4"
    },
    "require-dev": {
        "overtrue/phplint": "^4.0",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "lint": "phplint",
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "app": "php bin/app.php --ansi"
    }
}
